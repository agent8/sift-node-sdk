{
  "name": "siftapi",
  "version": "2.0.0",
  "author": "Edison",
  "main": "dist/siftapi.js",
  "module": "dist/siftapi.es6.js",
  "description": "A Node.js package for Edison Sift API.",
  "contributors": [
    {
      "name": "Bili Xu",
      "email": "bili@edison.tech"
    }
